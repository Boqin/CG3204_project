    .titlec {
    FONT-SIZE: 16px; VERTICAL-ALIGN: top
    }
    UL.execVideos LI {
    LIST-STYLE-TYPE: none; WIDTH: 160px; MARGIN-BOTTOM: 10px; FLOAT: left; COLOR: #000000; MARGIN-LEFT: 10px; FONT-SIZE: 12px; VERTICAL-ALIGN: middle; FONT-WEIGHT: bold
    }
    UL.execVideos {
    LIST-STYLE-TYPE: none; PADDING-LEFT: 0em; MARGIN-BOTTOM: 1em; MARGIN-LEFT: 1em
    }
