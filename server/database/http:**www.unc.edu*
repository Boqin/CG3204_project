	<script>
		jQuery(document).ready(function($){
			$('div.alertCarolinaMessage').prependTo('div#main').show();
			if ($.cookie('unc-bg') == 'on' || $.cookie('unc-bg') == null) {
				$('body').attr('class', 'bgon').css('background-image', 'url(http://uncmain.sites.unc.edu/files/2013/04/spring_04_12_11_oldwell.jpg)');
			}
			$('#about-bg-controls').fadeIn('slow');
			$('#bg-on-toggle').click(function() {
				$.cookie('unc-bg', 'on', { expires: 365, path: '/' });
				$('body').addClass('bgon').css('background-image', 'url(http://uncmain.sites.unc.edu/files/2013/04/spring_04_12_11_oldwell.jpg)');
			});
			$('#bg-off-toggle').click(function() {
				$.cookie('unc-bg', 'off', { expires: 365, path: '/' });
				$('body').removeClass('bgon').css('background-image', 'url(http://unc.edu/wp-content/themes/unc-main/images/background-off.jpg)');
			});
			$('#about-bg-show').click(function(){
				$('body').addClass('aboutbg');
			});
			$('#about-bg-hide').click(function() {
				$('body').removeClass('aboutbg');
			});
			$('#about-bg-minify').click(function() {
				$('#about-bg-info').toggleClass('minimize');
				if ($(this).html() == '+') {
					$(this).html('-');
				} else {
					$(this).html('+');
				}
			})
		});
	</script>
