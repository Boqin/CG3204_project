<script type="text/javascript">
function dosearch() {
 var sf=document.searchform;
  for (i=sf.sengines.length-1; i > -1; i--) {
  // if (!document.getElementById('b_one')) {
  // alert('foo is not set.'); 
  // var submitto = "http://www.unc.edu/search" + escape(sf.searchterms.value);
  // } else 
  if (sf.sengines[i].checked) {
    var submitto = sf.sengines[i].value + escape(sf.searchterms.value);
   }
  }
  window.location.href = submitto;
  return false;
}
</script>

