    <a name="511"></a><div id="dnn_ctr511_ContentPane" class="DNNAlignleft"> <!-- Start_Module_511 --><div id="dnn_ctr511_ModuleContent">
	<div id="dnn_ctr511_HtmlModule_HtmlModule_lblContent" class="Normal">
	<div class="BannerDiv_wrapper">
<script type="text/javascript"> 
	// var jQuery = jQuery.noConflict(); 
	jQuery(document).ready(function(){
		var shows = new Array();
		var imagesCount = 0;
	  jQuery.ajax({
		type: "GET",
		url: "/Portals/0/flash/flashBanner/image.xml",
		dataType: "xml",
		success: function(xml){
			var i = 0;
			jQuery(xml).find("photo").each(function(){
				shows[i] = new Array(4);
				shows[i][0] = jQuery(this).attr("image");
				shows[i][1] = jQuery(this).attr("link");
				shows[i][2] = jQuery(this).attr("alt");
				shows[i][3] = jQuery(this).attr("default");
				i++;
				imagesCount++;
				//jQuery('div#BannerDiv img').attr('src', jQuery(this).attr("image"));
				//alert(jQuery(this).attr("image"));
			})
			var j = 0;
			var start = 0;
			function slidShow()
			{
				if(shows)
				{
					jQuery('div#BannerDiv img').attr('src', '');	
					jQuery('div#BannerDiv img').css('padding-left', '0px');
					jQuery('div#BannerDiv img').css('padding-top', '0px');
					jQuery('div#BannerDiv img').attr('src', shows[j][0]);	
					jQuery('div#BannerDiv img').attr('alt', shows[j][2]);	
					jQuery('div#BannerDiv a').attr('href', shows[j][1]);
					j++;
					start++;
					if(j == imagesCount)
						j = 0;
					setTimeout(slidShow, ((start == 1 && shows[(j-1)][3] == 1) ? 2000 : 5000));
				}
			}
			slidShow();
		}
	  });
	});
</script>
<div id="BannerDiv" style="width: 609px; height: 218px; text-align: left;">
