    $(function() {
        $("table.m").each(function(index) {
            //var rowCount = $(this).find("tr").length;
            //alert("table: " + index + " rows: " + rowCount);
            $(this).find("tr").each(function(rowIndex) {
                $(this).find("td").each(function(colIndex) {
                    if (colIndex == 2) {
                        var str = $(this).html();
                    }
                    if (colIndex == 2) {
                        if (str.indexOf('yellow-arrow.gif') < 0)
                            $(this).html("&nbsp;");
                    }
                });
            });
        });
    });

</script>

