<script type="text/javascript">
$$('.egBG').each(function(eg){
	if($(eg).value!='')
		$(eg).removeClassName('egBG');
	eg.observe('focus',function(){this.removeClassName('egBG');});
	eg.observe('blur',function(){if(this.value=='')this.addClassName('egBG');});
})
