                                                        <h1>
                                                            <!-- InstanceBeginEditable name="title" -->
                                                            The NOC Experience
                                                            <!-- InstanceEndEditable -->
                                                        </h1>
                                                        <!-- InstanceBeginEditable name="content" -->
                                                        <br /><div align="center"><img src="images/Application_Closed_EDM.png" />
                                                        </div><!-- hide world map image with hotspots --><!-- <div align="center">
                                                            <img src="http://overseas.nus.edu.sg/images/areaPosition.gif" width="520" height="169" usemap="#Map1" border="0" />
                                                            <map name="Map1" id="Map1">
                                                                <area shape="rect" coords="145,23,209,50" href="colleges_shanghai_intro.htm" />
