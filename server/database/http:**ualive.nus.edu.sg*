<script type="text/javascript">
		$(function () {
			// Datepicker
			var disDays = [ "24/4/2013" , "26/3/2013" , "27/2/2013" , "21/1/2013" , "15/11/2012" , "31/10/2012" , "26/9/2012" , "29/8/2012" , "25/7/2012" , "13/6/2012" , "23/5/2012" , "25/4/2012" , "21/3/2012" , "08/2/2012" , "18/1/2012" , "30/11/2011" , "19/10/2011" , "28/9/2011" , "22/8/2011" , "27/7/2011" , "29/6/2011" , "27/4/2011" , "30/3/2011" , "17/2/2011" , "27/1/2011" ];
			var tdids = [ "a8b81cba-cb29-4e76-b9eb-1651fa0d0b86" , "637d5d70-9b14-4ec2-8775-4d51ec045c18" , "7ab44614-5852-4641-bd44-4eb6487fc2f9" , "d75ee39d-fc06-4187-9453-828e13379549" , "9a282434-c8f9-49cd-8098-7bf4819bb69a" , "23f81f36-7551-4842-83e7-9c1c96daf270" , "f1615e73-33c9-42ff-8163-ac1eb5b8d664" , "8a9c6ca9-929b-4dc8-bd8b-dc0f0bf1d2ca" , "52e3656c-749d-431a-96b6-d7aa7c825152" , "8a7dd1c1-88c6-4751-8996-14fb1aa63dd8" , "647f3415-cb94-4d04-bac2-9d5a1adfd290" , "9a6fb88a-7c3f-4e02-8e63-951ac5662e44" , "e30b6c36-71d6-4454-b0b6-d0c71fe30549" , "b5d6d834-a09b-4867-be9a-9490b18da501" , "16633e7d-b3a4-4692-9872-d7a59182e88d" , "1018418a-e3f7-4e92-91c1-a0c703acec79" , "0a477c98-9edb-4bec-8440-832f488c0f64" , "a2b76357-8c99-4a79-a809-77deaa172fb8" , "060cd82f-9358-4678-ac53-bc2aef4d977c" , "aed26d11-321a-42da-ba69-fd7c1e331833" , "f832101b-4d90-4fe3-82a3-6fc9867b2722" , "2a5f8b87-553c-45ee-ba9d-500ebf7d2d00" , "adb3288a-3c4d-4b6d-a644-7a954558052a" , "7c0b0109-bdb0-4359-b355-5bdd1fc9c375" , "ce2c9c7b-8568-4ec3-aa31-7d9a997c8dc5" ];
			function disableDays(date) {
				var sDate = date.getDate().toString() + "/" + (date.getMonth() + 1).toString() + "/" + date.getFullYear().toString();
				if ($.inArray(sDate, disDays) != -1) {
					return [true, ""];
				}
				else
					return [false, "daybooked", "Already Booked"];
			}

			$(document).ready(function () {
				$('#datepicker').datepicker({ inline: true, defaultDate: '17-02-11', altField: 'input#date', altFormat: 'dd-mm-yy', onSelect: function (dateText, inst) {
					var selectedDate = $(this).datepicker("getDate");
					var newDate = selectedDate.getDate().toString() + "/" + (selectedDate.getMonth() + 1).toString() + "/" + selectedDate.getFullYear().toString();
					var dateindex = $.inArray(newDate, disDays);
					for (var tdid in tdids) {
						$('#' + tdids[tdid].toString()).hide();
					}

					$('#' + tdids[dateindex].toString()).fadeIn(1000);
				}, beforeShowDay: disableDays
				});
			});


			//hover states on the static widgets
			$('#dialog_link, ul#icons li').hover(
					function () { $(this).addClass('ui-state-hover'); },
					function () { $(this).removeClass('ui-state-hover'); }
				);

		});

			

</script>
