<script type="text/javascript">
    $(document).ready(function () {
        $("div#welcomemsg").click(function () {
            hidechangenameitems();
            $("#changename, #changename-panel").fadeIn(300);
        })
        $("a#welcomemsg-close").click(function () {
            hidechangenameitems();
            $("#changename, #changename-panel").fadeOut(300);
        })

        function hidechangenameitems() {
            $('#changenameprogress').hide();
            $('#changenamesuccess').hide();
        }
    })
</script>


<script type="text/javascript">
    $(document).ready(function () {
        
        var options = {
            dataType: 'json',
            beforeSubmit: validateChangename,
            success: successChangename
        };

        $("#changenameform").ajaxForm(options);
        //$('#feedbackthanks').css('display', 'none');
        //$('feedbackthanks').css('display', 'none');
    });

    function validateChangename(formData, jqForm, options) {
        $('#changenameprogress').fadeIn(500);
        //$('#feedbackthanks').css('display', 'none');
        //$('#feedbackformandintro').css('display', 'none');
    }

    function successChangename(response, status) {
        if (status == 'success') {
            if (response.status == 'Success') {
                $('#changenameprogress').fadeOut(500);
                $('#changenamesuccess').fadeIn(500);
                $('#welcomemsg').html('Welcome, <b>' + response.firstName + ' ' + response.lastName + '</b>!');
            }
            //$('#feedbackthanks').css('display', 'inline');
            //$('#feedbackformandintro').css('display', 'none');
        }
    }
</script>

