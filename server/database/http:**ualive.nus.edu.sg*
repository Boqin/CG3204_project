                var optionsemail = {
                    dataType: 'json',
                    beforeSubmit: validatefeedbackemail,
                    success: successfeedbackemail
                };

                $("#emailsubscription").ajaxForm(optionsemail);
                $('#success-message').css('display', 'none');

            });

            function validatefeedbackemail(formData, jqForm, optionsemail) {
                if (!isValidEmailAddress($("#email").val())) {
                    $("#success-message").text("Please enter a valid email address");
                    $("#success-message").css('display', 'block');
                    return false;
                }
            }

            function successfeedbackemail(response, status) {
                if (status == 'success') {
                    //$("#success-message").text('Thank you for your interest! We\'ll keep you updated.');
                    $('#success-message').html('<p class="success" style="color: #9EBD2F; text-transform: uppercase;" id="success-messagepg"> Thank you for your interest! We\'ll keep you updated.</p>');
                    $('#success-message').css('display', 'block');
                    //$('#success-message').
                }
            }

            function isValidEmailAddress(emailAddress) {

                var pattern = new RegExp(/^(("[\w-\s]+")|([\w-]+(?:\.[\w-]+)*)|("[\w-\s]+")([\w-]+(?:\.[\w-]+)*))(@((?:[\w-]+\.)*\w[\w-]{0,66})\.([a-z]{2,6}(?:\.[a-z]{2})?)$)|(@\[?((25[0-5]\.|2[0-4][0-9]\.|1[0-9]{2}\.|[0-9]{1,2}\.))((25[0-5]|2[0-4][0-9]|1[0-9]{2}|[0-9]{1,2})\.){2}(25[0-5]|2[0-4][0-9]|1[0-9]{2}|[0-9]{1,2})\]?$)/i);

                return pattern.test(emailAddress);
            }
	</script>
