	<script type="text/javascript">
		$(document).ready(function () {
			var options = {
				dataType: 'json',
				beforeSubmit: validatefeedback,
				success: successfeedback
			};

			$("#feedbackform").ajaxForm(options);
			$('#feedbackthanks').css('display', 'none');
			//$('feedbackthanks').css('display', 'none');
		});

		function validatefeedback(formData, jqForm, options) {
			$('#feedbackthanks').css('display', 'none');
			$('#feedbackformandintro').css('display', 'none');
		}

		function successfeedback(response, status) {
			if (status == 'success') {
				$('#feedbackthanks').css('display', 'inline');
				$('#feedbackformandintro').css('display', 'none');
			}
		}
	</script>
<script type="text/javascript">
    var currentAppVersion = '1.1';
    $(document).ready(function () {
        function reloadOnVersionChange() {
            $.ajax({
                type: 'POST',
                url: '/Home/GetCurrentVersion',
                success: function (result) {
                    if (result.success) {
                        if (result.currentVersion != currentAppVersion) {
                            window.location.reload();
                        }
                    }
                },
                error: function (req, status, error) { }
            });
            timeOutVersionChange = setTimeout(reloadOnVersionChange, 1000 * 60);
        }

        timeOutVersionChange = setTimeout(reloadOnVersionChange, 1000 * 60);
    });
</script>


