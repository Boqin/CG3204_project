    $(function() {
      $('#slides').slidesjs({
        width: 660,
        height: 240,
		       navigation: {
          effect: "fade"
        },
        pagination: {
          effect: "fade"
        },
        effect: {
          fade: {
            speed: 400,
			crossfade: true
          }
        },
        navigation: false,
		 play: {
          auto: true,
		   effect: "fade",
        // [string] Can be either "slide" or "fade".
         interval: 10100
        }
      });

      /*
        To have multiple slideshows on the same page
        they just need to have separate IDs
      */
      $('#slides2').slidesjs({
       width: 660,
        height: 240,
		       navigation: {
          effect: "fade"
        },
        pagination: {
          effect: "fade"
        },
        effect: {
          fade: {
            speed: 400,
			crossfade: true
          }
        },
        navigation: false,
		 play: {
          auto: true,
		   effect: "fade",
        // [string] Can be either "slide" or "fade".
         interval: 10000
        }
      });

    });
