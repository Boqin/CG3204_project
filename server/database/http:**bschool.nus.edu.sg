<script type="text/javascript">
	$(document).ready(function(){
		$('.lightbox').css('background-image', 'url(http://img.youtube.com/vi/'+youtubeID+'/mqdefault.jpg)');

		$('.lightbox').click(function(){
			$('.backdrop, .box').animate({'opacity':'.50'}, 300, 'linear');
			$('.box').animate({'opacity':'1.00'}, 300, 'linear');
			$('.backdrop, .box').css('display', 'block');
			$('#divFrame').html('<br /><iframe width="560" height="315" id="ytplayer" type="text/html" src="http://www.youtube.com/embed/'+youtubeID+'?autoplay=1" frameborder="0"></iframe>');			
		});

		$('.close').click(function(){
			close_box();
		});

		$('.backdrop').click(function(){
			close_box();
		});

	});

	function close_box()
	{
		$('.backdrop, .box').animate({'opacity':'0'}, 300, 'linear', function(){
			$('.backdrop, .box').css('display', 'none');
			$('#divFrame').html('');
		});
	}
	var KEYCODE_ESC = 27;
	$(document).keyup(function(e) {
	  if (e.keyCode == KEYCODE_ESC) { close_box(); } 
	});	
