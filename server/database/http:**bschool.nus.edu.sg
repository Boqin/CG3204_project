<script type="text/javascript"> 
	// var jQuery = jQuery.noConflict(); 
	jQuery(document).ready(function(){
		var shows = new Array();
		var imagesCount = 0;
		var btnhtml='<ul id="btnnavlist">';
	  jQuery.ajax({
		type: "GET",
		url: "/Portals/0/flash/flashBanner/images.xml",
		dataType: "xml",
		success: function(xml){
			var i = 0;
			jQuery(xml).find("photo").each(function(){
				shows[i] = new Array(4);
				shows[i][0] = jQuery(this).attr("image");
				shows[i][1] = jQuery(this).attr("link");
				shows[i][2] = jQuery(this).attr("alt");
				shows[i][3] = jQuery(this).attr("default");
				i++;
				imagesCount++;
				//jQuery('div#BannerDiv img').attr('src', jQuery(this).attr("image"));
				//alert(jQuery(this).attr("image"));
				btnhtml = btnhtml+'<li>'+'<a href="#">'+imagesCount+'</a></li>';
			})
			btnhtml = btnhtml+'</ul>';
			

			var j = 0;
			var start = 0;
			var timeoutHandle;
			

			function slidShow()
			{

				if(shows)
				{
	
					jQuery("<img />").attr('src',shows[j][0]).load(function(){
					jQuery('div#BannerDiv img').css('padding-left', '0px');
					jQuery('div#BannerDiv img').css('padding-top', '0px');
					jQuery('div#BannerDiv img').attr('src', shows[j][0]);	
					jQuery('div#BannerDiv img').attr('alt', shows[j][2]);	
					jQuery('div#BannerDiv a').attr('href', shows[j][1]);
					j++;
					start++;
					if(j == imagesCount)
						j = 0;

					timeoutHandle = setTimeout(slidShow, ((start == 1 && shows[(j-1)][3] == 1) ? 2000 : 5000));
					});
				}
			}
			




			slidShow();
			

			jQuery('div#BannerBtn').html(btnhtml);
			

			jQuery('div#BannerContainer').hover(function(){
				jQuery('div#BannerBtn').show();
			},
			function(){
				jQuery('div#BannerBtn').hide();
			});
			

			jQuery('#btnnavlist > li').click(function(){
					var gotoVal = jQuery(this).children('a').html();
					j=gotoVal-1;
					jQuery("<img />").attr('src',shows[j][0]  + "?ramdom=" + new Date().getTime() ).load(function(){
					jQuery('div#BannerDiv img').css('padding-left', '0px');
					jQuery('div#BannerDiv img').css('padding-top', '0px');
					jQuery('div#BannerDiv img').attr('src', shows[j][0]);	
					jQuery('div#BannerDiv img').attr('alt', shows[j][2]);	
					jQuery('div#BannerDiv a').attr('href', shows[j][1]);
					j++;
					if(gotoVal == imagesCount)
						j = 0;

					clearTimeout(timeoutHandle);
					timeoutHandle = setTimeout(slidShow, ((start == 1 && shows[(j-1)][3] == 1) ? 2000 : 5000));
				});
return false;
					

			});
		}

	  });
	});
</script>
